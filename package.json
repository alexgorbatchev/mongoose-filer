{
    "name"            : "mongoose-filer"
  , "version"         : "0.0.11"
  , "description"     : "Upload, resize and crop images, upload to S3 and and attach them to Mongoose models with this easy to use plugin for Mongoose."
  , "homepage"        : "http://github.com/goodeggs/mongoose-filer"
  , "repository"      : {
      "type" : "git"
    , "url"  : "https://github.com/goodeggs/mongoose-filer.git"
  }
  , "keywords"        : []
  , "contributors"    : []
  , "licenses"        : ["MIT"]
  , "dependencies"    : {
      "async": "0.1.x"
    , "fs-extra": "0.1.x"
    , "imagemagick": "git://github.com/asalant/node-imagemagick.git#master"
    , "inflect" : "0.2.x"
    , "knox": "0.0.x"
    , "underscore": "1.3.x"
}
  , "devDependencies" : {
      "jasmine-node" : "git://github.com/asalant/jasmine-node.git#master"
    , "coffee-script" : "1.3.x"
    , "mongoose": "3.x"
  }
  , "main" : "lib/mongoose-filer"
  , "engine" : {
      "node" : ">=0.8"
  }
  , "scripts" : {
      "pretest" : "coffee -cl -o lib src"
    , "test" : "rm -rf ./tmp; jasmine-node --coffee --junitreport spec"
  }
}
